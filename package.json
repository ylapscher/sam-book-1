{
  "name": "sam-book",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.samstorybook.com/",
  "dependencies": {
    "@fillout/react": "^2.0.12",
    "@fontsource/oooh-baby": "^5.2.6",
    "@fontsource/pacifico": "^5.2.6",
    "@fontsource/satisfy": "^5.2.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "formik": "^2.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.3",
    "styled-components": "^6.1.19",
    "web-vitals": "^2.1.4",
    "yup": "^1.6.1"
  },
  "overrides": {
    "svgo": "^2.0.1",
    "postcss": "^8.4.38",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "CI=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@fillout/react|@fontsource)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "@fontsource/.*": "identity-obj-proxy"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "identity-obj-proxy": "^3.0.0"
  }
}
